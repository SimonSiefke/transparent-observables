{
  "name": "@relevant/compiler",
  "version": "0.0.4",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run test:generate && jest",
    "test:generate": "rimraf test/generated && node test/generate_tests.js",
    "test:watch": "npm run test:generate && jest --watch",
    "build": "rimraf dist && tsc",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "example:counter": "ts-node examples/counter/pre.ts && concurrently \"nodemon --ext ts,ui --ignore **/dist --exec ts-node examples/counter/pre.ts\" \"parcel examples/counter/src/counter.html\"",
    "example:todolist": "ts-node examples/todolist/pre.ts && concurrently \"nodemon --ext ts,ui --ignore **/dist --exec ts-node examples/todolist/pre.ts\" \"parcel examples/todolist/src/todolist.html\""
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "Simon Siefke",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.3",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-bitworkers": "^1.21.14",
    "jest": "^24.8.0",
    "parcel": "^1.12.3",
    "prettier": "^1.17.1",
    "prettier-config-bitworkers": "^1.5.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@relevant/ui": "^0.0.4"
  }
}
